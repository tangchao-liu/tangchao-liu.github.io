  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[CapitalDelta]", "\[LongEqual]", "0"}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\Delta = 0", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "10d20078-a25c-45a6-90a9-8b01d0abf2be"],
